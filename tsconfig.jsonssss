    "target": "es2016",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true /* Ensure that casing is correct in imports. */,
    "strict": true,
    "skipLibCheck": true
